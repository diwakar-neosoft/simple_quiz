<strong>Num Of Questions: <%= questions.size %></strong>
<table class='table'>
  <thead>
    <tr>
      <th>Title</th>
      <th>Input Type</th>
      <th>Options</th>
      <th>Answer</th>
      <th></th>
    </tr>
  </thead>
  <% questions.each do |question| %>
    <tbody>
      <tr>
        <td><%= question.title %></td>
        <td><%= question.inputType %></td>
        <td><%= question.optionValues.keys %></td>
        <td><%= question.answer %></td>
        <td><%= link_to 'show',question, class: 'btn btn-primary' %></td>
      </tr>
    </tbody>
  <% end %>
</table>